{
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.definitionLinkOpensInPeek": true,
    "editor.defaultFormatter": "NikolaosGeorgiou.html-fmt-vscode",
    "editor.experimentalInlineEdit.showToolbar": "always",
    "npm-intellisense.packageSubfoldersIntellisense": true,
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "code-runner.defaultLanguage": "HTML5",
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.runInTerminal": false,
    "code-runner.saveFileBeforeRun": true,
    "scss.showErrors": true,
    "[php]": {
        "editor.defaultFormatter": "DEVSENSE.phptools-vscode"
    },
    "json.format.enable": false,
    "gitHistory.logLevel": "None",
    "gitHistory.pageSize": 0,
    "css.completion.triggerPropertyValueCompletion": true,
    "gitlens.visualHistory.allowMultiple": false
}